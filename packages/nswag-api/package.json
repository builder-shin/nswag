{
  "name": "@aspect/nswag-api",
  "version": "0.0.1",
  "description": "생성된 OpenAPI 파일을 JSON/YAML 엔드포인트로 노출하는 미들웨어",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./express": {
      "types": "./dist/express.d.ts",
      "import": "./dist/express.js"
    },
    "./fastify": {
      "types": "./dist/fastify.d.ts",
      "import": "./dist/fastify.js"
    },
    "./nestjs": {
      "types": "./dist/nestjs.d.ts",
      "import": "./dist/nestjs.js"
    },
    "./koa": {
      "types": "./dist/koa.d.ts",
      "import": "./dist/koa.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsc -p tsconfig.build.json --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/koa": "^2.13.12",
    "fastify": "^4.0.0",
    "@nestjs/common": "^10.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "express": ">=4.0.0",
    "fastify": ">=4.0.0",
    "@nestjs/common": ">=10.0.0",
    "koa": ">=2.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "@nestjs/common": {
      "optional": true
    },
    "koa": {
      "optional": true
    }
  },
  "keywords": [
    "openapi",
    "swagger",
    "api-endpoint",
    "express",
    "fastify",
    "nestjs",
    "koa",
    "middleware"
  ],
  "author": "",
  "license": "MIT"
}
